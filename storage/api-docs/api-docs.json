    "paths": {
        "/transfer": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "Transfer",
                "description": "Transfer",
                "operationId": "authTransfer",
                "requestBody": {
                    "description": "transfer between accounts, only regular type users",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "Token",
                                    "value",
                                    "account"
                                ],
                                "properties": {
                                    "token": {
                                        "type": "string",
                                        "example": "14|YbQD8XXY4zSIOtR27xPj9jrM8w1GKekuDNKBx7OD"
                                    },
                                    "value": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "5"
                                    },
                                    "account": {
                                        "type": "string",
                                        "example": "3383585"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": {
                                                "status": "1",
                                                "message": "Transfer successfully completed.",
                                                "balance": 5
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
